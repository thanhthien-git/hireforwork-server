{
  "version": 2,
  "buildCommand": "go build -o main",
  "builds": [
    {
      "src": "*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/jobs",
      "dest": "/main"
    },
    {
      "src": "/companies",
      "dest": "/main"
    },
    {
      "src": "/careers",
      "dest": "/main"
    },
    {
      "src": "/tech",
      "dest": "/main"
    },
    {
      "src": "/field",
      "dest": "/main"
    },
    {
      "src": "/category",
      "dest": "/main"
    },
    {
      "src": "/admin",
      "dest": "/main"
    },
    {
      "src": "/jobs/(.*)",
      "dest": "/main"
    },
    {
      "src": "/companies/(.*)",
      "dest": "/main"
    },
    {
      "src": "/careers/(.*)",
      "dest": "/main"
    },
    {
      "src": "/tech/(.*)",
      "dest": "/main"
    },
    {
      "src": "/field/(.*)",
      "dest": "/main"
    },
    {
      "src": "/category/(.*)",
      "dest": "/main"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/main"
    },
    {
      "src": "/(.*)",
      "dest": "/main"
    }
  ]
}
